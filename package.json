{
  "name": "web-node-ide",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "copy-assets": "mkdir -p public/assets && cp node_modules/@sqlite.org/sqlite-wasm/dist/sqlite3.wasm public/assets/ && cp node_modules/@sqlite.org/sqlite-wasm/dist/sqlite3-opfs-async-proxy.js public/assets/",
    "prebuild": "npm run copy-assets"
  },
  "dependencies": {
    "@sqlite.org/sqlite-wasm": "^3.51.2-build6",
    "@webcontainer/api": "^1.6.1",
    "xstate": "^5.16.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@vitest/ui": "^2.0.4",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1",
    "vitest": "^2.0.4"
  }
}
