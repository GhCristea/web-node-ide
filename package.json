{
  "name": "web-node-ide",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ignore-pattern node_modules",
    "format": "prettier --write .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "copy-assets": "mkdir -p public/assets && cp node_modules/@sqlite.org/sqlite-wasm/dist/sqlite3.wasm public/assets/ && cp node_modules/@sqlite.org/sqlite-wasm/dist/sqlite3-opfs-async-proxy.js public/assets/",
    "prebuild": "npm run copy-assets"
  },
  "dependencies": {
    "@sqlite.org/sqlite-wasm": "^3.51.2-build6",
    "@webcontainer/api": "^1.6.1",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/xterm": "^6.0.0",
    "esbuild-wasm": "^0.27.3",
    "idb-keyval": "^6.2.2",
    "lucide-solid": "^0.563.0",
    "solid-js": "^1.9.11",
    "solid-monaco": "^0.3.0",
    "solid-resizable-panels": "^0.5.4",
    "solid-zustand": "^2.0.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/wicg-file-system-access": "^2023.10.7",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-unused-imports": "^4.4.1",
    "globals": "^16.5.0",
    "prettier": "^3.8.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "vite-plugin-solid": "^2.11.10"
  }
}
